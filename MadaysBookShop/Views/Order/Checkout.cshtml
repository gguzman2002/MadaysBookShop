@model Order

<form asp-action="Checkout" method="post" role="form">
    <h3>Get ready for some reading!</h3>

    <div asp-validation-summary="All"></div>

    <label asp-for="FirstName"></label>
    <input asp-for="FirstName" />

    <label asp-for="LastName"></label>
    <input asp-for="LastName" />

    <label asp-for="AddressLine1"></label>
    <input asp-for="AddressLine1" />

    <label asp-for="AddressLine2"></label>
    <input asp-for="AddressLine2" />

    <label asp-for="City"></label>
    <input asp-for="City" />

    <label asp-for="State"></label>
    <input asp-for="State" />

    <label asp-for="ZipCode"></label>
    <input asp-for="ZipCode" />

    <label asp-for="Country"></label>
    <input asp-for="Country" />

    <label asp-for="Email"></label>
    <input asp-for="Email" />

    <label asp-for="PhoneNumber"></label>
    <input asp-for="PhoneNumber" />

    <input type="submit" value="Complete order" />
</form>